{
  "name": "react-base",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development react-scripts start",
    "dev:staging": "env-cmd -f .env.staging react-scripts start",
    "dev:production": "env-cmd -f .env.production react-scripts start",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test --watchAll=false --no-cache",
    "test:coverage": "react-scripts test --coverage --watchAll=false --no-cache",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "npx pretty-quick --staged && npx lint-staged",
    "postinstall": "bit install"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "classnames": "^2.3.2",
    "csstype": "^3.1.2",
    "firebase": "^10.4.0",
    "i18next": "^23.4.4",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.43.8",
    "react-i18next": "^13.1.2",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "react-toastify": "^9.1.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.5.4",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-google-analytics": "^6.5.4",
    "workbox-navigation-preload": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-range-requests": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-streams": "^6.5.4",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "webpack": "5"
  }
}
