{
  "name": "reactjs-base",
  "scripts": {
    "prepare": "husky install",
    "changeset": "changeset",
    "bump": "changeset version",
    "do": "ts-node ./scripts/run-pnpm-command.ts",
    "is": "ts-node ./scripts/run-pnpm-command.ts -r i",
    "format": "npx pretty-quick --staged && npx lint-staged",
    "generate": "ts-node ./scripts/generate-package.ts",
    "dev": "ts-node ./scripts/run-pnpm-command.ts --stream --parallel dev",
    "dev:all": "pnpm --filter \"./apps/**\" --stream --parallel dev",
    "start": "ts-node ./scripts/run-pnpm-command.ts --stream --parallel start",
    "start:all": "pnpm --filter \"./apps/**\" --stream --parallel start",
    "build": "ts-node ./scripts/run-pnpm-command.ts -r build",
    "build:all": "pnpm --filter \"./packages/**/**\" -r build",
    "publish": "ts-node ./scripts/run-pnpm-command.ts -r publish --access public --no-git-checks",
    "publish:all": "pnpm --filter \"./packages/**/**\" -r publish --access public --no-git-checks",
    "rm:packages": "pnpm --filter \"./packages/**/**\" --filter \"./apps/**\" -r exec rimraf ./node_modules && rimraf ./node_modules && rimraf ./pnpm-lock.yaml",
    "rm:builds": "pnpm --filter \"./packages/**/**\" --filter \"./apps/**\" -r exec rimraf dist && pnpm --filter \"./packages/**/**\" --filter \"./apps/**\" -r exec rimraf build",
    "rm:all": "pnpm rm:packages && pnpm rm:builds",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@chromatic-com/storybook": "1.3.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@hookform/resolvers": "^3.4.2",
    "@storybook/addon-a11y": "^8.1.5",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-onboarding": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-vite": "^8.0.10",
    "@storybook/test": "^8.0.10",
    "@types/node": "^20.12.10",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "clsx": "^2.1.1",
    "csstype": "^3.1.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "firebase": "^10.11.1",
    "husky": "^9.0.11",
    "i18next": "^23.11.4",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-sort-imports": "^1.8.5",
    "pretty-quick": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.51.5",
    "react-i18next": "^14.1.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.0.10",
    "styled-components": "^6.1.10",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "yargs": "^17.7.2",
    "yup": "^1.4.0"
  }
}
